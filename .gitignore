# Ignore node_modules in both client and server
node_modules/
client/node_modules/
server/node_modules/

# Ignore environment variables
client/.env
server/.env

# Ignore logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore compiled or built files
client/build/
client/dist/
server/dist/
server/.cache/

# Ignore OS-specific files
.DS_Store
Thumbs.db


# Ignore temporary files
*.tmp
*.swp

# Ignore VsC-specific files
.vscode/

# Ignore database files (if any, e.g., SQLite for local testing)
*.sqlite
*.db

# Ignore Docker files if using Docker
docker-compose.override.yml
.dockerignore

# Ignore NGINX generated files (if using NGINX)
nginx/*.conf.backup
